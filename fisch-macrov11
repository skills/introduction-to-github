CameraCheckLeft := ScreenWidthLeft+(Width/2.1694)
CameraCheckRight := ScreenWidthLeft+(Width/1.8550)
CameraCheckTop := ScreenHeightTop+(Height/1.1612)
CameraCheckBottom := ScreenHeightTop+Height
