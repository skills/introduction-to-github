</tw-passagedata><tw-passagedata pid="108" name="走廊" tags="" position="2125,2025" size="100,100">&lt;&lt;link &quot;前进&quot;&gt;&gt;
    &lt;&lt;set $pressCount += 1&gt;&gt;

    &lt;&lt;if $pressCount == 1&gt;&gt;
        &lt;&lt;replace &quot;#mainText&quot;&gt;&gt;你继续往前走&lt;&lt;/replace&gt;&gt;
    &lt;&lt;/if&gt;&gt;

    &lt;&lt;if $pressCount == 2&gt;&gt;
        &lt;&lt;replace &quot;#mainText&quot;&gt;&gt;你越深入周围就越暗，似乎进入了黑夜似的&lt;&lt;/replace&gt;&gt;
    &lt;&lt;/if&gt;&gt;

    &lt;&lt;if $pressCount == 3&gt;&gt;
        &lt;&lt;replace &quot;#mainText&quot;&gt;&gt;你找到了一处阴暗的凉亭...&lt;&lt;/replace&gt;&gt;
    &lt;&lt;/if&gt;&gt;

    &lt;&lt;if $pressCount &gt; 3&gt;&gt;
        &lt;&lt;goto &quot;凉亭&quot;&gt;&gt;
    &lt;&lt;/if&gt;&gt;
